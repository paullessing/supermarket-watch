{
  "name": "shoppi",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "serve:all": "concurrently -n \"backend,frontend\" -c \"cyan,yellow\" \"yarn serve:api:dev\" \"yarn serve:frontend:dev\"",
    "serve:api:dev": "nx serve api",
    "serve:frontend:dev": "nx serve frontend --host=0.0.0.0",
    "start": "nx serve",
    "start:api": "node dist/apps/api/main.js",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "lint:all": "nx workspace-lint && ng affected:lint",
    "pre-push": "yarn lint:all && yarn test && yarn build --prod",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "if [ \"$NODE_ENV\" != \"production\" ]; then node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main; fi",
    "dc-prod": "docker-compose -f docker/docker-compose.prod.yml",
    "build:proxy": "cd proxy && yarn install",
    "start:proxy": "cd proxy && yarn start",
    "db:restore": "docker compose exec -T mongo mongoimport --drop -d shopping -c priceComparisons < ./db_backups/priceComparisons.bson && docker compose exec -T mongo mongoimport --drop -d shopping -c productHistory < ./db_backups/productHistory.bson"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.0",
    "@angular/cdk": "~13.2.0",
    "@angular/common": "~13.2.0",
    "@angular/compiler": "~13.2.0",
    "@angular/core": "~13.2.0",
    "@angular/forms": "~13.2.0",
    "@angular/platform-browser": "~13.2.0",
    "@angular/platform-browser-dynamic": "~13.2.0",
    "@angular/router": "~13.2.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "1.0.2",
    "@nestjs/serve-static": "2.2.2",
    "@swimlane/ngx-charts": "^20.1.0",
    "axios": "^0.21.2",
    "axios-cookiejar-support": "^0.5.1",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^2.5.4",
    "date-fns": "^2.21.2",
    "fast-deep-equal": "^3.1.3",
    "jest-extended": "^2.0.0",
    "mongodb": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "winston": "^3.7.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.2.0",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.2.0",
    "@angular/compiler-cli": "13.2.0",
    "@angular/language-service": "13.2.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/angular": "13.7.1",
    "@nrwl/cli": "13.7.1",
    "@nrwl/eslint-plugin-nx": "13.7.1",
    "@nrwl/jest": "13.7.1",
    "@nrwl/linter": "13.7.1",
    "@nrwl/nest": "13.7.1",
    "@nrwl/node": "13.7.1",
    "@nrwl/tao": "13.7.1",
    "@nrwl/workspace": "13.7.1",
    "@types/cheerio": "^0.22.15",
    "@types/cron": "^1.7.2",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "^5.10.2",
    "concurrently": "^5.0.2",
    "dotenv": "10.0.0",
    "eslint": "~8.7.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "27.2.3",
    "jest-preset-angular": "11.0.0",
    "prettier": "2.5.1",
    "replayer": "^2.2.3",
    "ts-jest": "27.0.5",
    "ts-node": "~7.0.0",
    "typescript": "4.5.5"
  }
}
